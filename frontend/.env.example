# Taxi Fare Prediction Frontend - Environment Configuration
# Copy this file to .env and update the values as needed

# API Configuration
REACT_APP_API_URL=http://localhost:8000

# Map Configuration (if using external map services)
# REACT_APP_MAP_API_KEY=your_map_api_key_here

# Application Configuration
REACT_APP_APP_NAME=Taxi Fare Prediction
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false

# External Services (if needed)
# REACT_APP_SENTRY_DSN=your_sentry_dsn_here
# REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
